<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sistema de cadastro de Produto</title>
    <link rel="stylesheet" href="/bootstrap/css/bootstrap.css">
</head>
<body>
    <nav class="navbar navbar-expand-sm navbar-light bg-light">
        <div class="container">
            <a href="/" class="navbar-brand">HOME</a>
            <div class="collapse navbar-collapse">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a href="/produtos" class="nav-link">Cadastrar Produto</a>
                    </li>
                    <li class="nav-item">
                        <a href="/produtos/buscar" class="nav-link">Buscar Produto</a>
                    </li>

                </ul>
            </div>
        </div>
    </nav>
   <div class="container">
    <div class="row">
        <div class="col-12">
            <% if(typeof messages != "undefined" && messages && messages.length > 0){ %>
                <div class="row">
                 <div class="col-12">
                    <div class="alert alert-info">
                        <% messages.forEach((msg) => { %>
                            <p>
                                <%=msg%>
                            </p>
            
                       <% }) %>
                    </div>
                 </div>   
                </div>
            <% } %>